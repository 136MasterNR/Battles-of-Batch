    IF EXIST "%Folder2%!RelPath!" (
      >NUL 2>&1 FC /b "%Folder1%!RelPath!" "%Folder2%!RelPath!" && (
       ECHO.Verified - %%x >>".\data\logs\verify-file-identity.log"
