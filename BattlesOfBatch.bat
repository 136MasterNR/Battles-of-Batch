    )
)

FOR /R "%Folder2%" %%x IN (*.*) DO (

    SET "FullPath=%%x"
    SET "RelPath=!FullPath:%Folder2%=!"

    IF NOT EXIST "%Folder1%!RelPath!" (
      ECHO.Missing - %Folder1%!RelPath! >>".\data\logs\verify-file-identity.log"
	  SET /A MIS.CNT+=1
    )
)
RD /S /Q "%EXTRAC.LOC%"

ECHO.Found %NEW.CNT% new files.
