:MERGE
set FRAME=[H

TITLE PHASE-1-0

for /f "tokens=1,* delims==" %%1 in ('set FRAME.') do (
	CALL :MANIPULATE %%1 "%%2"
)

TITLE PHASE-2-0

for /L %%I in (1, 1, %LINES%) do (
	TITLE PHASE-2-1
	for /f "tokens=1,* delims==" %%1 in ('set FRAME.LINE-%%I.LAYER- 2^>NUL') do (
		TITLE PHASE-2-2
		for /f "delims=" %%A in ('call echo %%FRAME%%[G%%2') do (
			TITLE PHASE-2-3
			set "FRAME=%%A"
		)
	)
	TITLE PHASE-2-4
	CALL SET FRAME=%%FRAME%%[1B[G
)

echo.%FRAME%
EXIT /B 0

:MANIPULATE
SET TMP=%2
SET TMP=%TMP:"=%
SET %1=%TMP: =[1C%
EXIT /B 0
