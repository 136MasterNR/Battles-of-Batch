SET /P BUILD=<"%DATA_SCRIPTS%\list\item_id.txt"
BREAK>"%DATA_TMP%\tmprun.cmd"
SETLOCAL ENABLEDELAYEDEXPANSION
SET COUNT=0
FOR %%A IN (%BUILD:;= %) DO (
	SHIFT
	SET /A COUNT=COUNT+1
	SET "LET_LIST.!COUNT!=%%~A"
)
SET COUNT=0
FOR %%A IN (%LET_LIST.1:[= %) DO (
	SHIFT
	SET /A COUNT=COUNT+1
	::ECHO.[!COUNT!] '%%~A'>>WATCH.txt
	ECHO.SET "ITEM.!COUNT!=%%~A">>"%DATA_TMP%\tmprun.cmd"
)
SET COUNT=0
FOR %%A IN (%LET_LIST.2:[= %) DO (
	SHIFT
	SET /A COUNT=COUNT+1
	::ECHO.[!COUNT!] '%%~A'>>WATCH.txt
	ECHO.SET "ITEM_LVL.!COUNT!=%%~A">>"%DATA_TMP%\tmprun.cmd"
)
SET COUNT=0
FOR %%A IN (%LET_LIST.3:[= %) DO (
	SHIFT
	SET /A COUNT=COUNT+1
	SET "MAT.%COUNT%=%%~A"
)
SET COUNTX=0
FOR /F %%B IN ('SET MAT.') DO CALL :MAT %%B
SET MAT.>LIST.txt
PAUSE
ENDLOCAL
CALL "%DATA_TMP%\tmprun.cmd"
DEL /Q "%DATA_TMP%\tmprun.cmd"
GOTO :EOF
:MAT

	SET /A COUNTX=COUNTX+1
	ECHO.SET "MAT.!COUNT!.!COUNTX!=%1">>"%DATA_TMP%\tmprun.cmd"