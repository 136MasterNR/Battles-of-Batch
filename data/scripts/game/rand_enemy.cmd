SET SMALLEST_HP=
FOR /F "TOKENS=4,5 DELIMS=.=" %%1 IN ('SET ENEMY.HP.NOW.') DO (
	IF NOT "%%1"=="T" IF NOT %%2 EQU 0 IF NOT DEFINED SMALLEST_HP (
		SET SMALLEST_HP=%%2
		SET INPUTATK=%%1
	) ELSE (
		CALL :IF %%1 %%2
	)
)
TITLE %INPUTATK%
EXIT /B 0
:IF
IF %2 LSS %SMALLEST_HP% (
	SET SMALLEST_HP=%2
	SET INPUTATK=%1
)
EXIT /B 0